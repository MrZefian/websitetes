{ 
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {}
    },
    "rules": {
        "no-console": 0,
        "semi": "warn",
        "no-unused-vars": "warn",
        "no-undef": "warn"
    }
}